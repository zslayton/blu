sub bool_string(expr)
	if expr then
		print "true"
	else
		print "false"
	end
end

write "1 < 5 --> "
bool_string(1 < 5)

write "1 == 2 --> "
bool_string(1 == 2)

write "1 < 5 and 1 == 2 --> "
bool_string(1 < 5 and 1 == 2)

write "1 < 5 or 1 == 2 --> "
bool_string(1 < 5 or 1 == 2)

write '"dog" == "dog" --> '
bool_string("dog" == "dog")

write '"dog" == "cat" --> '
bool_string("dog" == "cat")
