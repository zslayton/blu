sub greet(name)
	print "Hey ", name, "!"
end

greet("Fibonacci")

sub add(v1, v2)
	print v1, " + ", v2, " = ", (v1 + v2)
end

add(1, 1)
add(1, 2)
add(2, 3)
add(3, 5)
add(5, 8)

sub print_formal_name(name)
	print "Mr. ", name
end

sub formal_greet(name)
	write "Good day, "
	print_formal_name(name)
end

formal_greet("Fibonacci")
