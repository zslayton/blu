x = 10
y = 6

print "Globals:"
print "global x = ", x
print "global y = ", y

print "--------------------------"
print "Entering lexical 'x' block"
do
	local x = 42
	print "lexical x = ", x
end

print "--------------------------"
print "Entering lexical 'y' block"
do
	local y = 2
	print "lexical y = ", y
end

print "--------------------------"
print "Globals:"
print "global x = ", x
print "global y = ", y
