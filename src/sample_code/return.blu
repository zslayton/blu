fn wrap(str, tag)
	return "<"..tag..">\n"..str.."\n</"..tag..">"
end

fn table(str)
	return wrap(str, 'table')
end

fn col(str)
	return wrap(str, 'td')
end

fn row(str)
	return wrap(str, 'tr')
end

fn body(str)
	return wrap(str, 'body')
end

fn html(str)
	return wrap(str, 'html')
end

cells = ['a', 'b', 'c', 'd', 'e', 'f']

var markup = ''
x = 0
while x < len(cells) do
	markup = markup .. row(col(cells[x]))
	x = x + 1
end

markup = html(body(table(markup)))
print(markup)
